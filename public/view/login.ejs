<body class="hold-transition login-page">
<div class="login-box">


    <div class="preloader flex-column justify-content-center align-items-center">
        <img class="animation__shake" src="../img/logo.png" alt="AdminLTELogo" height="60" width="60">
        <h5>Setting up things for you ...</h5>
      </div>
  <!-- /.login-logo -->
 
  <div class="card card-outline card-primary">
      <div class="card-header text-center">
        <div class="row d-flex justify-content-center">
            <img src="../img/logo.png" class=""  alt="...">
        </div>
        <a href="../ui/index2.html" class="h1">Alumni Monitoring System</a>
        <div class="row">
        </div>
    </div>
    <div class="card-body">
      <p class="login-box-msg">Sign in to start your session</p>

      <form action="../ui/index3.html" method="post">
        <div class="input-group mb-3">
          <input type="text" class="form-control" placeholder="User">
          <div class="input-group-append">
            <div class="input-group-text">
              <span class="fas fa-user"></span>
            </div>
          </div>
        </div>
        <div class="input-group mb-3">
          <input type="password" class="form-control" placeholder="Password">
          <div class="input-group-append">
            <div class="input-group-text">
              <span class="fas fa-lock"></span>
            </div>
          </div>
        </div>
        <div class="row">

          <!-- /.col -->
          <div class="col-12">
            <button type="submit" class="btn btn-primary btn-block">Sign In</button>
          </div>
          <!-- /.col -->
        </div>
      </form>



    </div>
    <!-- /.card-body -->
  </div>
  <!-- /.card -->
</div>
